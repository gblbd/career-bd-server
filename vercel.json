{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@now/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "http://careersbangladesh.com"
      },
      "dest": "index.html"
    }
  ]
}
