{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://careersbangladesh.com"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "https://careersbangladesh.com"
        }
    ],
    "public": true,
    "crons": [
        {
            "path": "/cron",
            "schedule": "0 10 * * *"
        }
    ]
}